{
  "name": "Lifevision",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "postversion": "react-native-version",
    "postinstall": "rm -f ./node_modules/react-native/local-cli/core/__fixtures__/files/package.json",
    "codepush:production": "code-push release-react Lifevision-1 Android -d Production & code-push release-react Lifevision iOS -d Production",
    "codepush:staging": "code-push release-react Lifevision-1 Android -d Staging & code-push release-react Lifevision iOS -d Staging",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier-eslint \"src/**/*.js\" --write --single-quote ",
    "test": "jest ./tests",
    "build-e2e:ios": "detox build -c ios.sim.debug",
    "test-e2e:ios": "detox test -c ios.sim.debug -l verbose",
    "build-e2e:android": "yarn clean:android && detox build -c android.emu.debug",
    "test-e2e:android": "detox test -c android.emu.debug -l verbose",
    "test-env": "cp config.test.example.json config.json",
    "test:ios": "yarn test-env && yarn build-e2e:ios && yarn test-e2e:ios",
    "test:android": "yarn test-env && yarn test-e2e:android",
    "run:android": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-android",
    "run:android:release": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-android --variant=release",
    "run:android:staging": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-android --variant=stagingRelease",
    "run:ios": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-ios",
    "debug:android": "node node_modules/react-native/local-cli/cli.js log-android",
    "clean:android": "cd android && gradlew clean && cd ..",
    "app:link": "node node_modules/react-native/local-cli/cli.js link @react-native-community/blur",
    "fetch-cms-static": "babel-node scripts/contentful-fetch.js --output ../src/static/cms.json --presets es2015,stage-2,react-native",
    "migrate-db": "babel-node scripts/migrate-database --output step5.json --presets es2015,stage-2,react-native"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@react-native-community/async-storage": "^1.4.2",
    "@react-native-community/blur": "^3.3.1",
    "@react-native-community/slider": "^1.1.3",
    "apollo-client-preset": "^1.0.5",
    "autobind-decorator": "^2.1.0",
    "contentful": "^4.6.1",
    "date-fns": "^1.29.0",
    "deep-equal": "^1.0.1",
    "entypo": "^2.1.0",
    "evil-icons": "^1.10.1",
    "global": "^4.4.0",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "invariant": "^2.2.3",
    "md5": "^2.2.1",
    "metro-react-native-babel-preset": "0.51.0",
    "moment": "^2.20.1",
    "pluralize": "^7.0.0",
    "ramda": "^0.25.0",
    "react": "16.8.3",
    "react-apollo": "^2.0.4",
    "react-composer": "^5.0.0",
    "react-connect-context": "^1.1.0",
    "react-native": "0.59.8",
    "react-native-animatable": "^1.2.4",
    "react-native-cli": "^2.0.1",
    "react-native-code-push": "^5.4.2",
    "react-native-contacts": "^3.1.1",
    "react-native-device-info": "^0.13.0",
    "react-native-elements": "^0.19.1",
    "react-native-emoji-selector": "^0.1.6",
    "react-native-fbsdk": "0.8.0",
    "react-native-gesture-handler": "1.1.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-linear-gradient": "^2.5.3",
    "react-native-markdown-syntax": "^0.1.0",
    "react-native-material-dialog": "^0.7.6",
    "react-native-modal-selector": "^1.0.0",
    "react-native-pages": "^0.6.1",
    "react-native-progress": "^3.6.0",
    "react-native-push-notification": "^3.1.1",
    "react-native-select-input-ios": "^1.1.4",
    "react-native-sentry": "0.36.0",
    "react-native-slider": "^0.11.0",
    "react-native-snap-carousel": "3.6.0",
    "react-native-sound": "0.10.5",
    "react-native-svg": "9.4.0",
    "react-native-svg-charts": "^5.2.0",
    "react-native-svg-icon": "^0.8.1",
    "react-native-svg-image": "^2.0.0",
    "react-native-swiper": "^1.5.13",
    "react-native-typography": "^1.2.1",
    "react-native-unified-contacts": "git://github.com/joshuapinter/react-native-unified-contacts.git#master",
    "react-native-vector-icons": "^6.5.0",
    "react-native-video": "^3.2.0",
    "react-native-webview": "5.0.1",
    "react-navigation": "1.5.2",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-enhancer-react-native-appstate": "^0.3.0",
    "redux-persist": "^5.4.0",
    "redux-reset": "^0.3.0",
    "redux-saga": "1.0.0-beta.0",
    "redux-storage": "^4.1.2",
    "redux-thunk": "^2.2.0",
    "simple-markdown": "^0.3.1",
    "tcomb-form-native": "^0.6.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-native": "^5.0.2",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.18.1",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.69.0",
    "jest": "^23.6.0",
    "jest-serializer-enzyme": "^1.0.0",
    "jest-static-stubs": "^0.0.1",
    "mocha": "^3.5.3",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.5.2",
    "react-native-version": "^2.4.1",
    "react-test-renderer": "16.6.3",
    "reactotron-react-native": "^2.0.0-alpha.1",
    "reactotron-redux": "^2.0.0",
    "reactotron-redux-saga": "^2.0.0",
    "redux-test-utils": "^0.3.0"
  },
  "rnpm": {
    "assets": [
      "./src/resources"
    ]
  },
  "jest": {
    "preset": "react-native",
    "snapshotSerializers": [
      "./node_modules/jest-serializer-enzyme"
    ]
  },
  "detox": {
    "specs": "__e2e__",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Lifevision.app",
        "build": "xcodebuild -project ios/Lifevision.xcodeproj -scheme Lifevision -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 5s"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "build": "pushd android && gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5_API_25"
      }
    }
  }
}
