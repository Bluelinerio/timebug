    "redux-storage": "^4.1.2",
    "redux-storage-decorator-debounce": "^1.1.3",
    "redux-storage-decorator-filter": "^1.1.8",
    "redux-storage-engine-reactnativeasyncstorage": "^1.0.5",
    "tcomb-form-native": "^0.6.10"
  },
  "devDependencies": {
    "babel-preset-react-native": "1.9.2",
    "detox": "5.3.0",
    "flow-bin": "^0.55.0",
    "mocha": "^3.5.3",
    "react-test-renderer": "16.0.0-alpha.6"
  },
  "detox": {
    "specs": "__e2e__",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/book2020.app",
        "build": "xcodebuild -project ios/book2020.xcodeproj -scheme book2020 -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 5s"
      }
    }
  }
