{
  "name": "2020",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-e2e:ios": "detox build -c ios.sim.debug",
    "test-e2e:ios": "detox test -c ios.sim.debug -l verbose",
    "build-e2e:android": "yarn clean:android && detox build -c android.emu.debug",
    "test-e2e:android": "detox test -c android.emu.debug -l verbose",
    "test-env": "cp config.test.example.json config.json",
    "test:ios": "yarn test-env && yarn build-e2e:ios && yarn test-e2e:ios",
    "test:android": "yarn test-env && yarn test-e2e:android",
    "run:android": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-android",
    "run:ios": "cp config.example.json config.json && node node_modules/react-native/local-cli/cli.js run-ios",
    "clean:android": "cd android && gradlew clean && cd ..",
    "fetch-cms-static": "node scripts/contentful-fetch.js --exec babel-node --presets es2015, stage-2"
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.2.1",
    "apollo-client": "^1.9.3",
    "autobind-decorator": "^2.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "contentful": "^4.6.1",
    "gql": "^1.1.2",
    "graphql-tag": "^2.4.2",
    "react": "16.2.0",
    "react-apollo": "^1.4.16",
    "react-native": "0.50.1",
    "react-native-button": "^2.1.0",
    "react-native-fbsdk": "0.6.3",
    "react-native-image-header-scroll-view": "^0.6.1",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-keyboard-spacer": "^0.4.1",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-markdown-syntax": "^0.1.0",
    "react-native-pages": "^0.6.1",
    "react-native-parallax-scroll-view": "^0.20.1",
    "react-native-snap-carousel": "^3.4.0",
    "react-native-svg-image": "^2.0.0",
    "react-native-swiper": "^1.5.12",
    "react-native-theme": "^0.1.8",
    "react-native-vector-icons": "^4.4.0",
    "react-navigation": "git://github.com/ex1machina/react-navigation.git",
    "react-redux": "^5.0.6",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "1.0.0-beta.0",
    "redux-storage": "^4.1.2",
    "redux-storage-decorator-debounce": "^1.1.3",
    "redux-storage-decorator-filter": "^1.1.8",
    "redux-storage-engine-reactnativeasyncstorage": "^1.0.5",
    "simple-markdown": "^0.3.1",
    "tcomb-form-native": "^0.6.11"
  },
  "devDependencies": {
    "babel-preset-react-native": "1.9.2",
    "detox": "^5.10.0",
    "flow-bin": "0.60.1",
    "mocha": "^3.5.3",
    "react-test-renderer": "16.0.0-alpha.6",
    "remote-redux-devtools": "^0.5.12"
  },
  "detox": {
    "specs": "__e2e__",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/2020.app",
        "build": "xcodebuild -project ios/2020.xcodeproj -scheme 2020 -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 5s"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/app-debug.apk",
        "build": "pushd android && gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5_API_25"
      }
    }
  }
}
