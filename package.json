    "react": "16.0.0-alpha.6",
    "react-native": "0.44.2",
    "react-native-button": "^2.1.0",
    "react-native-easy-markdown": "^1.1.8",
    "react-native-fbsdk": "0.6.0",
    "react-native-image-header-scroll-view": "^0.6.1",
    "react-native-linear-gradient": "^2.3.0",
    "react-native-markdown-syntax": "^0.1.0",
    "react-native-swiper": "^1.5.12",
    "react-native-theme": "^0.1.8",
    "react-native-vector-icons": "^4.4.0",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
