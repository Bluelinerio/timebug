            },
          ],
          "audio": Object {
            "uri": "https://assets.ctfassets.net/6h184bey8vl3/1KsYCYGg20mWSYomu2QK4A/0974e76bc0da6d0c9d91263faa221dc2/20-20-19-Phase-1-Step-8.mp3",
          },
          "checkin": Object {
