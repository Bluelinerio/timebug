            "audio": Object {
              "uri": "https://assets.ctfassets.net/6h184bey8vl3/387A7bSjqMmYwkuGaY8sqg/708caa66cc4aaa3e1709f351afddac44/20-20-14-Phase-1-Step-5.mp3",
